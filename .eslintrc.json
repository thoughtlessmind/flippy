{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier/"
    ],
    "rules": {
        "valid-jsdoc": "warn",
        "require-jsdoc": "warn",
        "linebreak-style":"off"
    },
    "settings": {
        "react": {
            "version": "latest"
        }
    }
}